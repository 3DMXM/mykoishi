{
  "name": "@koishijs/boilerplate",
  "version": "0.0.0",
  "packageManager": "yarn@4.0.1",
  "private": true,
  "files": [
    ".env",
    "koishi.yml"
  ],
  "license": "AGPL-3.0",
  "workspaces": [
    "plugins/Adapter/*",
    "plugins/AI/*",
    "plugins/Behavior/*",
    "plugins/Console/*",
    "plugins/Extension/*",
    "plugins/Games/*",
    "plugins/Manager/*",
    "plugins/News/*",
    "plugins/Recreation/*",
    "plugins/Tool/*",
    "plugins/@*/*",
    "external/wechaty-satori-sdk/packages/adapter-wechat4u",
    "external/open-vits",
    "external/test"
  ],
  "scripts": {
    "new": "koishi-scripts new",
    "setup": "koishi-scripts setup",
    "clone": "koishi-scripts clone",
    "build": "yakumo build",
    "bump": "yakumo version",
    "dep": "yakumo upgrade",
    "pub": "yakumo publish",
    "dev": "cross-env NODE_ENV=development koishi start -r esbuild-register -r yml-register",
    "start": "koishi start",
    "set-proxy": "npm config set registry https://registry.npmmirror.com",
    "unset-proxy": "npm config delete registry",
    "test:gh-tile": "mocha --exit -r esbuild-register -r yml-register Plugins/News/gh-tile/test/index.spec.ts",
    "test:gh-tile:cron": "node -r esbuild-register Plugins/News/gh-tile/test/cron.spec.ts",
    "test:jimp": "node -r esbuild-register Plugins/Tool/jimp/test/base.spec.ts"
  },
  "devDependencies": {
    "@koishijs/client": "^5.20.1",
    "@koishijs/plugin-hmr": "^1.2.6",
    "@koishijs/scripts": "^4.5.0",
    "@types/node": "^20.10.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.20",
    "esbuild-register": "npm:@shigma/esbuild-register@^1.1.1",
    "typescript": "^5.3.2",
    "yakumo": "^1.0.0-alpha.4",
    "yakumo-esbuild": "^1.0.0-alpha.0",
    "yakumo-esbuild-yaml": "^1.0.0-alpha.0",
    "yakumo-publish-sync": "^1.0.0-alpha.1",
    "yakumo-tsc": "^1.0.0-alpha.1",
    "yml-register": "^1.1.0"
  },
  "dependencies": {
    "@koishijs/plugin-actions": "^0.0.2",
    "@koishijs/plugin-adapter-satori": "^1.0.3",
    "@koishijs/plugin-admin": "^2.0.0-alpha.5",
    "@koishijs/plugin-analytics": "2.0.2",
    "@koishijs/plugin-auth": "^4.1.6",
    "@koishijs/plugin-bind": "^1.4.5",
    "@koishijs/plugin-commands": "^3.4.3",
    "@koishijs/plugin-config": "2.6.0",
    "@koishijs/plugin-console": "5.21.0",
    "@koishijs/plugin-database-mysql": "5.7.4",
    "@koishijs/plugin-database-sqlite": "3.8.3",
    "@koishijs/plugin-explorer": "^1.5.0",
    "@koishijs/plugin-help": "^2.4.1",
    "@koishijs/plugin-insight": "3.4.4",
    "@koishijs/plugin-inspect": "^1.1.3",
    "@koishijs/plugin-locales": "^2.5.1",
    "@koishijs/plugin-logger": "2.6.1",
    "@koishijs/plugin-market": "2.7.0",
    "@koishijs/plugin-oobe": "^0.0.2",
    "@koishijs/plugin-sandbox": "^3.2.2",
    "@koishijs/plugin-server": "^3.1.3",
    "@koishijs/plugin-server-satori": "^2.4.0",
    "@koishijs/plugin-server-temp": "^1.2.0",
    "@koishijs/plugin-status": "7.3.0",
    "koishi": "4.16.1",
    "koishi-plugin-android": "^0.0.1",
    "koishi-plugin-assets-local": "^3.2.0",
    "koishi-plugin-dataview": "2.6.1",
    "koishi-plugin-desktop": "^0.0.4",
    "koishi-plugin-puppeteer": "^3.7.3",
    "koishi-plugin-rate-limit": "^2.0.1",
    "koishi-plugin-telemetry": "^0.0.2",
    "koishi-plugin-theme-vanilla": "^1.0.6"
  }
}
